{
  "name": "ember-cli-styles-namespace",
  "version": "1.0.0-alpha.7",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/webark/ember-cli-styles.git",
    "directory": "packages/namespace"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prettier:write": "prettier --write .",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test --test-port=7359",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-persistent-filter": "^3.1.2",
    "broccoli-tree-walker": "^1.0.1",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-cli-styles-colocation": "^1.0.0-alpha.4",
    "fs-extra": "^10.0.0",
    "md5": "^2.3.0",
    "postcss": "^8.4.6",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.3",
    "postcss-selector-namespace": "^3.0.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^1.2.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-less-single": ">= 2.0.1",
    "broccoli-postcss-single": ">= 5.0.1",
    "broccoli-sass-source-maps": ">= 4.0.0",
    "broccoli-stylus-single": ">= 1.0.1",
    "ember-auto-import": "^2.4.0",
    "ember-cli": "~4.1.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-styles-preprocessor": "^1.0.0-alpha.4",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.2.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "loader.js": "^4.7.0",
    "no-style-files-yet": "link:./tests/dummy/lib/no-style-files-yet",
    "npm-run-all": "^4.1.5",
    "postcss-import": ">= 14.0.2",
    "postcss-preset-env": ">= 7.4.2",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "sass": ">= 1.49.9",
    "second-test-addon": "link:./tests/dummy/lib/second-test-addon",
    "test-addon": "link:./tests/dummy/lib/test-addon",
    "webpack": "^5.68.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "before": [
      "ember-cli-babel",
      "ember-cli-styles-preprocessor",
      "ember-cli-less",
      "ember-cli-sass",
      "ember-cli-stylus",
      "ember-cli-postcss"
    ],
    "after": [
      "ember-cli-styles-colocation"
    ],
    "configPath": "tests/dummy/config"
  }
}
