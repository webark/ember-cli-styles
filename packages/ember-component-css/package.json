{
  "name": "ember-component-css",
  "version": "0.2.4",
  "description": "An Ember CLI addon which allows you to specify CSS inside of component pod directories",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "Erik Bryn",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
