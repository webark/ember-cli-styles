    "fs-tree-diff": "^0.5.4",
    "md5": "^2.1.0",
    "postcss": "^5.2.6",
    "postcss-less": "^0.15.0",
    "postcss-scss": "^0.4.0",
    "postcss-selector-namespace": "^1.2.4",
    "rsvp": "^3.2.1",
    "walk-sync": "^0.3.1"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
