    "start": "ember server",
    "test": "ember try:each",
    "prepublish": "./bin/install-test-addons.sh"
  },
  "dependencies": {
    "broccoli-concat": "^3.0.5",
    "broccoli-funnel": "^1.0.9",
    "broccoli-merge-trees": "^1.1.5",
    "broccoli-persistent-filter": "^1.2.6",
    "broccoli-plugin": "^1.2.1",
    "ember-cli-babel": "^5.1.7",
