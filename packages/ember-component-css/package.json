{
  "name": "ember-component-css",
  "version": "0.1.5",
  "description": "An Ember CLI addon which allows you to specify CSS inside of component pod directories",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "test": "ember test && mocha tests/lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ebryn/ember-component-css.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Erik Bryn",
  "license": "MIT",
  "devDependencies": {
    "broccoli": "^0.16.2",
    "broccoli-asset-rev": "^2.0.0",
    "ember-cli": "^0.2.3",
    "ember-cli-app-version": "0.3.1",
    "ember-cli-babel": "^4.0.0",
    "ember-cli-content-security-policy": "0.3.0",
    "ember-cli-dependency-checker": "0.0.7",
    "ember-cli-htmlbars": "0.7.4",
    "ember-cli-ic-ajax": "0.1.1",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-qunit": "0.3.8",
    "ember-cli-uglify": "1.0.1",
    "ember-data": "1.0.0-beta.15",
    "ember-export-application-global": "^1.0.2",
    "expect.js": "^0.3.1",
    "express": "^4.8.5",
    "glob": "^4.0.5",
    "mocha": "^2.2.4"
  },
  "keywords": [
    "ember-addon"
  ],
  "ember-addon": {
    "before": [
      "ember-cli-sass",
      "ember-cli-less",
      "ember-cli-stylus"
    ],
    "configPath": "tests/dummy/config"
  },
  "dependencies": {
    "broccoli-funnel": "^0.2.2",
    "broccoli-merge-trees": "^0.2.1",
    "broccoli-writer": "^0.1.1",
    "css": "^2.2.0",
    "ember-cli-preprocess-registry": "^1.0.1",
    "ember-cli-version-checker": "^1.0.2",
    "symlink-or-copy": "^1.0.1",
    "walk-sync": "^0.1.3"
  }
}
