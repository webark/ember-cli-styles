{
  "name": "ember-component-css",
  "version": "1.0.0-alpha.1",
  "description": "An Ember CLI addon which allows you to specify styles for individual components",
  "keywords": [
    "ember-addon"
  ],
  "repository": "github:ebryn/ember-component-css",
  "license": "MIT",
  "author": "Erik Bryn",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "release": "standard-version",
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "broccoli-babel-transpiler": "^7.2.0",
    "broccoli-file-manifest": "^2.0.2",
    "broccoli-funnel": "^2.0.2",
    "broccoli-merge-trees": "^3.0.2",
    "broccoli-persistent-filter": "^2.3.1",
    "broccoli-style-manifest": "^1.5.2",
    "broccoli-tree-walker": "^1.0.0",
    "ember-cli-babel": "^7.11.0",
    "fs-extra": "^8.1.0",
    "md5": "^2.2.1",
    "postcss": "^7.0.17",
    "postcss-less": "^3.1.4",
    "postcss-scss": "^2.0.0",
    "postcss-selector-namespace": "^3.0.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.0.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.12.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars": "^3.1.0",
    "ember-cli-htmlbars-inline-precompile": "^3.0.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-styles-preprocessor": "^0.5.5",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.5.1",
    "ember-resolver": "^5.2.1",
    "ember-source": "~3.13.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.2.1",
    "eslint-plugin-ember": "^7.1.0",
    "eslint-plugin-node": "^10.0.0",
    "loader.js": "^4.7.0",
    "no-style-files-yet": "link:./tests/dummy/lib/no-style-files-yet",
    "qunit-dom": "^0.9.0",
    "second-test-addon": "link:./tests/dummy/lib/second-test-addon",
    "standard-version": "^7.0.0",
    "test-addon": "link:./tests/dummy/lib/test-addon"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "before": [
      "ember-cli-babel",
      "ember-cli-styles-preprocessor",
      "ember-cli-less",
      "ember-cli-sass",
      "ember-cli-stylus",
      "ember-cli-postcss"
    ],
    "configPath": "tests/dummy/config"
  }
}
